<script>
  let check = true;
  function shownav() {
    let leftSide = document.querySelector(".header--left");
    if (check) {
      leftSide.classList.add("res__header--left");
    } else {
      leftSide.classList.remove("res__header--left");
    }
    check = !check;
  }

  $(document).ready(function () {
    $(".slider").slick({
      autoplay: true,
      autoplaySpeed: 5000,
      infinite: true,
      prevArrow: '<button class="slide-arrow prev-arrow"></button>',
      nextArrow: '<button class="slide-arrow next-arrow"></button>',
    });

    $(".silder2").slick({
      slidesToShow: 2,
      slidesToScroll: 1,
      speed: 900,
      prevArrow: '<button class="slide-arrow prev-arrow"></button>',
      nextArrow: '<button class="slide-arrow next-arrow"></button>',
    });
  });
</script>
